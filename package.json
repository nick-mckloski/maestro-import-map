{
    "name": "maestro-import-map",
    "type": "module",
    "version": "1.0.0",
    "scripts": {
        "prepare": "husky",
        "compile": "tsc --noEmit",
        "lint": "eslint --config eslint.config.js --fix",
        "lint:check": "eslint --config eslint.config.js",
        "lint:report": "eslint --config eslint.config.js -f stylish",
        "format": "prettier . --write",
        "format:check": "prettier . --check"
    },
    "lint-staged": {
        "./**": [
            "npm run lint",
            "npm run format"
        ]
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "eslint": "^9.39.2",
        "husky": "^9.1.7",
        "prettier": "^3.7.4",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.0"
    },
    "dependencies": {
        "@tealium/shared-dependencies": "^1.7.0",
        "lint": "^0.8.19",
        "staged": "^0.0.0"
    }
}
